<?xml version="1.0"?>
<launch>
  <!-- send table urdf to param server -->
  <param name="pin_description" command="$(find xacro)/xacro $(find ur_e_description)/urdf/environment/box_hole_pin/pin.xacro" />
 
  <!-- push table_description to factory and spawn robot in gazebo -->
  <node name="spawn_pin" pkg="gazebo_ros" type="spawn_model" 
    args="-urdf 
          -param pin_description 
          -x 0.15
          -y 0.4
          -z 0.96
          -R 0
          -P 0
          -Y 0
          -model pin" 
    respawn="false" output="screen" />


</launch>
