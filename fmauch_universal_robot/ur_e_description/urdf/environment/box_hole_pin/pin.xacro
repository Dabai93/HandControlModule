        iyy="0.007293" iyz="0.0"
        izz="0.00392"/>
    </inertial> -->
  </link>
  
  
    <joint name="pin_joint" type="floating">
    <parent link="world"/>
    <child link="pin_link"/>
    <origin xyz="0.15 0.4 0" rpy="0 0 0"/>
  </joint>
  
    <gazebo reference="pin_link">
    <material>Gazebo/Red</material>
    <selfCollide>true</selfCollide>
    <mu1>100</mu1>  
    <mu2>100</mu2>
    <kp>100000000</kp>
    <kd>1</kd>  
    <minDepth>0.00001</minDepth>  
    <maxVel>0.0001</maxVel> 
  </gazebo>
 

