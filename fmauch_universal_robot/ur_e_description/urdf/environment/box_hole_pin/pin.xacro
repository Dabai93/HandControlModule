  <link name="pin_link">
    <collision>
      <geometry>
        <mesh filename="package://ur_e_description/meshes/Boxwithholeandpin/pin.stl" scale="1 1 1"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <mesh filename="package://ur_e_description/meshes/Boxwithholeandpin/pin.stl" scale="1 1 1"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="0.1"/>
      <inertia
        ixx="0.0"
        ixy="0.0" 
        ixz="0.0"
        iyy="0.0" 
        iyz="0.0"
        izz="0.0"/>
    </inertial>
  <!--  <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
