<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="LegoBlue">

  <link name="world">
  </link>

  <joint name="world_joint" type="fixed">
    <parent link="world"/>
    <child link="legostein_link"/>
    <axis xyz="1 1 1" />
    <dynamics damping="0.5" friction="0.5"/>
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
  </joint>

  <link name="legostein_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.031 0.08 0.031"/>   <!--0.024-->
      </geometry>
      <mu value="0.5"/>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.031 0.031 0.031"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <inertia
        ixx="0.001" ixy="0.0" ixz="0.0"
        iyy="0.001" iyz="0.0"
        izz="0.001"/>
    </inertial>
    <self_collide>1</self_collide>
  </link>

  <gazebo reference="legostein_link">
    <material>Gazebo/Blue</material>
  </gazebo>

</robot>
