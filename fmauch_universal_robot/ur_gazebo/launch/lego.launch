<?xml version="1.0"?>
<launch>
  <!-- send table urdf to param server -->
  <param name="lego_description" command="$(find xacro)/xacro.py $(find ur_description)/urdf/environment/lego.xacro" />
 
  <!-- push table_description to factory and spawn robot in gazebo -->
  <node name="spawn_lego" pkg="gazebo_ros" type="spawn_model" 
    args="-urdf 
          -param lego_description 
          -z 0.001 
          -model lego" 
    respawn="false" output="screen" />

</launch>
